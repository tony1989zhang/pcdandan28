    <style name="CustomDialog" parent="@android:style/Theme.Dialog">
        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 背景透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 模糊 -->
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="push_style">
        <item name="android:hint">@string/style_hint</item>
        <item name="android:inputType">number</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="push_alias">
        <item name="android:hint">@string/alias_hint</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="push_tag">
        <item name="android:hint">@string/tag_hint</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="tv_twostart" >
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:background">@drawable/selector_tv_tra</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="tv_title_twostart">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginLeft">5dp</item>

    </style>
    <style name="vg_twostart">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">2</item>
        <!--<item name="com.demo.demo_2:isRadio">true</item>-->
        <item name="android:background">@color/white</item>
        <item name="android:gravity">center</item>
       <item name="com.xlidfwsscai525:singleLine">true</item>
        <item name="com.xlidfwsscai525:tagMargin">5dp</item>
        <item name="android:layout_marginLeft">3dp</item>
    </style>

    <style name="layout_fm0_ll">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/background_view_rounded_middle</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="layout_fm0_tv">
        <item name="android:paddingTop">5dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">@dimen/text_default_size</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="layout_fm0_tv2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/line</item>
        <item name="android:textSize">@dimen/text_smail_size</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="list_item_middle">
        <item name="android:clickable">true</item>
        <item name="android:paddingTop">10dip</item>
        <item name="android:paddingBottom">10dip</item>
        <item name="android:paddingLeft">10dip</item>
        <item name="android:paddingRight">10dip</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/background_view_rounded_middle</item>
    </style>
